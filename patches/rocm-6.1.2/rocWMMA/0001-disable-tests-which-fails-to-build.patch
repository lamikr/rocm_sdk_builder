+  option( ROCWMMA_BUILD_TESTS "Build rocWMMA tests" OFF )
   option( ROCWMMA_BUILD_SAMPLES "Build rocWMMA samples" ON )
   option( ROCWMMA_BUILD_ASSEMBLY "Output assembly files" OFF )
 endif()
