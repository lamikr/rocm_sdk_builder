     )
   endif()
 
diff --git a/library/include/rocrand/rocrand_common.h b/library/include/rocrand/rocrand_common.h
index 2f16c6e..0701f18 100644
--- a/library/include/rocrand/rocrand_common.h
+++ b/library/include/rocrand/rocrand_common.h
@@ -103,7 +103,23 @@ namespace detail {
       defined(__gfx906__) || \
       defined(__gfx908__) || \
       defined(__gfx909__) || \
-      defined(__gfx1030__) )
+      defined(__gfx1010__) || \
+      defined(__gfx1011__) || \
+      defined(__gfx1012__) || \
+      defined(__gfx1030__) || \
+      defined(__gfx1031__) || \
+      defined(__gfx1032__) || \
+      defined(__gfx1034__) || \
+      defined(__gfx1035__) || \
+      defined(__gfx1036__) || \
+      defined(__gfx1100__) || \
+      defined(__gfx1101__) || \
+      defined(__gfx1102__) || \
+      defined(__gfx1103__) || \
+      defined(__gfx1150__) || \
+      defined(__gfx1151__) || \
+      defined(__gfx1200__) || \
+      defined(__gfx1201__) )
   #if !defined(ROCRAND_ENABLE_INLINE_ASM)
     #define ROCRAND_ENABLE_INLINE_ASM
   #endif
