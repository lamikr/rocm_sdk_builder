BINFO_APP_NO_BUILD=1
BINFO_APP_NO_POSTINSTALL=1

BINFO_APP_INSTALL_CMD_ARRAY=(
    "cd ${BINFO_APP_SRC_DIR}"
    "./install_rocm.sh ${INSTALL_DIR_PREFIX_SDK_ROOT} ${BINFO_APP_SRC_DIR}"
)
